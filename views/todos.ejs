<span id="count-<%= title %>" style="float:right; font-weight: bolder;"> Total :  <%= data.length %> </span>
<h5 class="text-lg" style="font-family: 'Times New Roman', Times, serif;"><%= title %></h5>
<ul class="list-none">
    <% for(var i=0; i<data.length; i++) { %> 
    <li class="Todo-item">
        <div class="hover:bg-rose-600 flex-none items-center rounded w-full  my-2 px-2 py-1">
            <input id = "todo-checkbox-<%= data[i].id %>" type="checkbox" <%= data[i].completed ? "checked" : "" %> onclick="updateTodo(<%= data[i].id %>, <%= !data[i].completed %>)" class="w-4 h-4 rounded">
            <label for="todo-checkbox-<%= data[i].id %>" class="text-lg cursor-pointer mx-2"><%= data[i].title %></label>
            <a href="#" style="float:right" onclick="deleteTodo(`<%= data[i].id %>`)" class="hidden trash-icon">
                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke-width="1.5" stroke="currentColor" class="w-6 h-6">
<path stroke-linecap="round" stroke-linejoin="round" d="M9.75 9.75l4.5 4.5m0-4.5l-4.5 4.5M21 12a9 9 0 11-18 0 9 9 0 0118 0z" />
</svg>

            </a>

        </div>                        
    </li>
    <% } %>
</ul>